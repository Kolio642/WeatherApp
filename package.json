{
  "name": "weather-website",
  "version": "1.0.0",
  "description": "A weather website that provides weather information by city",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "npm install && mkdir -p dist && cp -r *.js *.html *.css .env.example dist/",
    "deploy": "npm run build"
  },
  "keywords": [
    "weather",
    "weatherapi",
    "nodejs",
    "express"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "node-cache": "^5.1.2",
    "node-fetch": "^2.6.12"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
} 